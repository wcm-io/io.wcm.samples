<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:cloudserviceconfigs="[[]]"
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/apps/wcm-io-samples/core/templates/content/homepage"
        jcr:primaryType="cq:PageContent"
        jcr:title="en"
        sling:resourceType="wcm-io-samples/core/components/content/page/homepage"
        includeAside="{Boolean}true"
        includeAsideBar="{Boolean}true"
        includeTeaserBar="{Boolean}true"
        includeTeaserbar="{Boolean}true"
        inheritAside="{Boolean}false"
        inheritTeaserbar="{Boolean}false"
        navTitle="HOME"
        pageTitle="wcm.io Sample Site">
        <teaserbar
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm-io-samples/core/components/content/teaserbar/teaserbarParsys">
            <teaserbaritem
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/teaserbar/teaserbarItem"
                linkContentRef="/content/wcm-io-samples/en/conference"
                linkMediaDownload="{Boolean}false"
                linkTitle="This should help you with your decision"
                linkType="internal"
                linkWindowFeatures="default"
                linkWindowTarget="_self"
                mediaRef="/content/dam/wcm-io-samples/content/user.png"
                teaserContent="Still not convinced to attend? Need persuasion? Facts for your boss?"
                title="Why to attend"/>
            <teaserbaritem_0
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/teaserbar/teaserbarItem"
                linkContentRef="/content/wcm-io-samples/en/venue"
                linkTitle="More information"
                linkType="internal"
                linkWikipediaLanguage="en"
                linkWindowFeatures="default"
                linkWindowTarget="_self"
                mediaRef="/content/dam/wcm-io-samples/content/test-video.mp4"
                teaserContent="Take a look at the new venue. The Kulturbrauerei in the Prenzlauer Berg district."
                title="Video Teaser"/>
            <teaserbaritem_1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/teaserbar/teaserbarItem"
                linkContentRef="/content/wcm-io-samples/en/conference/call-for-papers"
                linkExternalRef="https://adapt.to"
                linkTitle="External Link"
                linkType="external"
                linkWindowFeatures="default"
                linkWindowTarget="_blank"
                mediaRef="/content/dam/wcm-io-samples/content/talk.png"
                teaserContent="If you have insight and experiences with Apache Sling and want to share them? We are actually asking for your participation!"
                title="Want to share?"/>
            <teaserbaritem_2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/teaserbar/teaserbarItem"
                linkContentRef="/content/wcm-io-samples/en/archive"
                linkMediaDownload="{Boolean}true"
                linkMediaRef="/content/dam/wcm-io-samples/content/sample.pdf"
                linkTitle="Download PDF"
                linkType="media"
                linkWindowFeatures="default"
                linkWindowTarget="_self"
                mediaRef="/content/dam/wcm-io-samples/content/wcm-io.svg"
                teaserContent="adaptTo() is not a new event. Take a look at what was said and done previously."
                title="Take a look back"/>
        </teaserbar>
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm-io-samples/core/components/structure/parsys/asideParsys">
            <asidesponsorteaser
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/aside/asideSponsorTeaser"
                title="Sponsors">
                <images
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io-samples/core/components/structure/parsys/asideParsys">
                    <asidesponsorteaserit_0
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io-samples/core/components/content/aside/asideSponsorTeaserItem"
                        imageHeight="41"
                        imageWidth="200"
                        linkExternalRef="http://www.pro-vision.de"
                        linkType="external"
                        linkWindowFeatures="default"
                        linkWindowTarget="_blank"
                        mediaRef="/content/dam/wcm-io-samples/content/provision-logo.png"/>
                </images>
            </asidesponsorteaser>
            <asidesocialteaser
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/aside/asideSocialTeaser"
                title="Follow us">
                <images
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io-samples/core/components/structure/parsys/asideParsys">
                    <asidesocialteaserite
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io-samples/core/components/content/aside/asideSocialTeaserItem"
                        linkExternalRef="http://twitter.com/adaptto"
                        linkMediaDownload="{Boolean}false"
                        linkTitle="@adaptTo"
                        linkType="external"
                        linkWindowFeatures="default"
                        linkWindowTarget="_blank"
                        mediaRef="/content/dam/wcm-io-samples/content/twitter-icon.png"
                        title="on twitter"/>
                </images>
            </asidesocialteaser>
        </aside>
        <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm-io/wcm/parsys/components/parsys">
            <contentrichtext
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/common/contentRichText"
                text="&lt;p&gt;adaptTo() is a meetup in Berlin focused on Apache Sling including Apache Jackrabbit and Apache Felix and is addressed to all using this stack or parts of it.&lt;/p&gt;&#xD;&#xA;&lt;p&gt;&lt;a data=&quot;{&amp;quot;linkType&amp;quot;:&amp;quot;internal&amp;quot;,&amp;quot;linkContentRef&amp;quot;:&amp;quot;/content/wcm-io-samples/handler/en/conference&amp;quot;,&amp;quot;linkWindowTarget&amp;quot;:&amp;quot;_self&amp;quot;,&amp;quot;linkWindowFeatures&amp;quot;:&amp;quot;default&amp;quot;}&quot; href=&quot;#&quot; data-link-type=&quot;internal&quot; data-link-content-ref=&quot;/content/wcm-io-samples/en/conference&quot; data-link-external-ref=&quot;#&quot; data-link-window-target=&quot;_self&quot;&gt;Read more...&lt;/a&gt;&lt;/p&gt;&#xD;&#xA;"
                textIsRich="true"/>
            <contentheadline
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/common/contentHeadline"
                headline="Extended Call for Papers"
                smaller="{Boolean}true"/>
            <contentrichtext_0
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/common/contentRichText"
                text="&lt;p&gt;Although we got some great submissions for adaptTo(), we still have some slots for further sessions. Therefore we extend the timeslot for submissions to the call for papers and for feedback by two weeks. This means you still can submit you submissions till 06.05. We're looking forward to get more of your great talks.&lt;/p&gt;&#xD;&#xA;&lt;p&gt;&lt;a data=&quot;{&amp;quot;linkType&amp;quot;:&amp;quot;internal&amp;quot;,&amp;quot;linkContentRef&amp;quot;:&amp;quot;/content/wcm-io-samples/handler/en/conference/call-for-papers&amp;quot;,&amp;quot;linkWindowTarget&amp;quot;:&amp;quot;_self&amp;quot;,&amp;quot;linkWindowFeatures&amp;quot;:&amp;quot;default&amp;quot;}&quot; href=&quot;#&quot; data-link-type=&quot;media&quot; data-link-content-ref=&quot;/content/wcm-io-samples/en/conference&quot; data-link-external-ref=&quot;#&quot; data-link-window-target=&quot;_self&quot; data-link-media-ref=&quot;/content/dam/wcm-io-samples/content/sample.pdf&quot; data-link-media-download=&quot;true&quot;&gt;Read more...&lt;/a&gt;&lt;/p&gt;&#xD;&#xA;"
                textIsRich="true"/>
        </content>
        <stage
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm-io/wcm/parsys/components/parsys">
            <stageheader
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm-io-samples/core/components/content/stage/stageheader"
                linkMediaDownload="{Boolean}false"
                linkType="internal"
                linkWindowFeatures="default"
                linkWindowTarget="_self"
                mediaRef="/content/dam/wcm-io-samples/content/stageheader-outside2.jpg"
                subtitle="Kulturbrauerei Berlin"
                title="adaptTo()">
                <links
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm-io-samples/core/components/content/stage/stageheaderParsys">
                    <stageheaderlinkitem
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io-samples/core/components/content/stage/stageheaderLinkItem"
                        linkContentRef="/content/wcm-io-samples/en/conference"
                        linkTitle="Get tickets now"
                        linkType="internal"
                        linkWindowFeatures="default"
                        linkWindowTarget="_self"/>
                    <stageheaderlinkitem_0
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm-io-samples/core/components/content/stage/stageheaderLinkItem"
                        linkContentRef="/content/wcm-io-samples/en/conference/call-for-papers"
                        linkMediaDownload="{Boolean}false"
                        linkTitle="Submit paper"
                        linkType="internal"
                        linkWindowFeatures="default"
                        linkWindowTarget="_self"/>
                </links>
            </stageheader>
        </stage>
        <image jcr:primaryType="nt:unstructured"/>
    </jcr:content>
    <tools/>
    <conference/>
</jcr:root>
